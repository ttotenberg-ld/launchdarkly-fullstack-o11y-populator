# LaunchDarkly Client-Side ID (not secret - safe to expose)
VITE_LD_CLIENT_SIDE_ID=your-client-side-id

# API Gateway URL
VITE_API_URL=http://localhost:5000

# Error Injection Settings (for observability demo)
# Enable/disable error injection (default: true)
VITE_ERROR_INJECTION_ENABLED=true
# Probability of injecting errors per session (0.0-1.0, default: 0.05 = 5%)
VITE_ERROR_INJECTION_RATE=0.05
